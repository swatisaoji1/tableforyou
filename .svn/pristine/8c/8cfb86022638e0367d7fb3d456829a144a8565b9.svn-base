@media (max-width: 979px) {
    body {
        padding-top: 0px;
    }
}


#header{
    background-color: red;
    height: 20px;
}

#footer{
    background-color: blue;
    height: 20px;
}

.restaurant{
    border : 1px solid red;
    width: 100%;

}

.red{
    border : 1px solid red;
}

#bootstrap-overrides h4{
    color: red;
}

.restaurant-entry{
    background-color: rgb(253,250,253);
    margin: 0px 0px 0px 0px;
    padding: 0px 0px 0px 0px;
    height: inherit;
}

.restaurant-entry-image{
    max-height: 170px;
    width: 100%
}

.restaurant-entry-detail-container{
    padding: 10px 10px 5px 25px;
}

.restaurant-entry-reserve-form{
    border-width: 0px;
    box-shadow: none;
}

.restaurant-entry-item{
    margin-right: 5%;
}

.t4u_restaurant_detail{
    margin-left: 5%;
}
.t4u_restaurant-entry-row{
    margin: 0px 0px 10px 0px;
}
.searchbar{
    padding: 10px;
}

.regContainer{
    width: 970px;
}

.loginForm{
    width: 400px;
    margin: auto;
}

.center {
    margin: auto;
    width: 50%;
    padding: 10px;
}

.forgotMessage{
    text-align: center;
}

.nav a {
    color: #5a5a5a;
    font-size: 15px;
    font-weight: bold;
    padding: 14px 10px;
    text-transform: uppercase;
}

.nav li {
    display: inline;
}

.t4u_navbar {
    margin-bottom: 0px;
    background-color: white;
    background-image: none;
    box-shadow: none;
}

#innerserach{
    padding-top: 20px;
    padding-bottom: 20px;
}

.required_marker{
    color: red;
}