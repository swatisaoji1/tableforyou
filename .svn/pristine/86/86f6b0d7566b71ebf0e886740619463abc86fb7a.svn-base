/* Homepage */
.t4u_searchbar_row{
    height: 100%;
}

.table4u_background {
    background-image:url('http://chaine-cambodia.com/wp-content/uploads/2013/12/empty-glass-dining-table-image-8.jpg');
    background-position: 0% 40%;
    height: 600px;
    width: 100%;
    top:100px;
    background-repeat: no-repeat;
    margin-bottom: 0px;
}

.t4u_homepage_text {
    color: #fff;
    font-size: 40px;  
    font-family: 'Dancing Script', cursive;
    font-weight: bold;
    //position: relative;
}

.t4u_searchbar {
    height: 50px;
    margin-top: 150px;
    font-size: 18px;
    margin-left: 10%; 
    margin-right: 10%;
}

.t4u_searchcol {
    height: 100%;
    padding: 0px 0px 0px 0px;
}

.t4u_searchbarSearchInput {
    height: 100%;
    font-size: 16px;
    position: relative;
    width: 100%;
    padding: 8px 20px 8px 20px;
    border-width: 0px;
    border-left-width: 1px;
    background-color: rgb(248,248,248);
    vertical-align: top;
}

.t4u_searchbarButton{
    height: 100%;
    width: 100%;
    background-color: #FF3366;
    font-size: 16px;
    position: relative;
    transition: 1s;
    color: white;
    padding: 8px 20px 8px 20px;
    border-width: 0px;
    border-left-width: 1px;
    border-radius: 0;
    vertical-align: top;
}

.t4u_searchbarButton:hover{
    background-color: #e6005c;
    color: white;
}

