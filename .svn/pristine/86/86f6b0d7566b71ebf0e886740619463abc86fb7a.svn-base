/* Search Result Page */
.t4u_searchbar_resultpage {
    height: 50px;
    //margin-top: 20px;
    font-size: 18px;
    margin-left: 10%; 
    margin-right: 10%;
    background-color: rgb(250,250,250);
    border-style: solid;
    border-width: 1px 1px 1px 1px;
    border-color: rgb(200,200,200);

}

.t4u_searchcol_resultpage {
    height: 100%;
    padding: 0px 0px 0px 0px;
    background-color: rgb(248,248,248);
    border-right-style: solid;
    border-right-width: 1px;
    border-color: rgb(200,200,200);

}

.t4u_searchbarPeopleSelect {
    height: 100%;
    width: 100%;
    font-size: 16px;
    padding: 8px 20px 8px 20px;
    border-width: 0px;
    border-left-width: 0px;
}

.t4u_searchbarTimeSelect {
    height: 100%;
    width: 100%;
    font-size: 16px;
    padding: 8px 20px 8px 20px;
    border-width: 0px;
    border-left-width: 0px;
}

.t4u_calendarSelect {
    height: 100%;
    width: 88%;
    font-size: 16px;
    position: relative;    
    border-width: 0px;
    background-color: rgb(248,248,248);
    padding: 8px 20px 8px 20px;
}

.t4u_calendarSelect::-webkit-calendar-picker-indicator{
    display: none;
}
.t4u_calendarSelect[type="date"]::-webkit-input-placeholder{ 
    visibility: hidden !important;
}

.t4u_searchbarSearchInput_resultpage {
    height: 100%;
    font-size: 16px;
    position: relative;
    width: 100%;
    padding: 8px 20px 8px 20px;
    border-width: 0px;
    //border-left-width: 1px;
    background-color: rgb(248,248,248);
    vertical-align: top;
}

.t4u_searchbarButton_resultpage{
    height: 100%;
    width: 100%;
    background-color: #FF3366;
    font-size: 16px;
    position: relative;
    transition: 1s;
    color: white;
    padding: 8px 20px 8px 20px;
    border-width: 0px;
    border-left-width: 1px;
    border-radius: 0;
    vertical-align: top;
}

.t4u_searchbar_row_resultpage{
    height: 100%;
}

